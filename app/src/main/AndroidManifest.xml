<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="inspectplus.dpwgroup.com.inspectplus">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.ProjectHomeActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activities.NewActionsActivity"
            android:label="NewActionsActivity"></activity>
        <activity
            android:name=".activities.RegisteredProjectsActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".activities.InspectionEventsActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>
